<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:paddingLeft="@dimen/_20px_in720p"
    android:paddingRight="@dimen/_20px_in720p">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_20px_in720p"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/context"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="对方未接听电话"
            android:textSize="@dimen/_28px_in720p" />

        <TextView
            android:id="@+id/yue"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1.2"
            android:gravity="right|center"
            android:text="2017-2-6 10:22:36"
            android:textColor="#cccccc"
            android:textSize="@dimen/_24px_in720p" />


    </LinearLayout>

    <TextView
        android:id="@+id/yuanyin"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_10px_in720p"
        android:text="打电话没人接听"
        android:textColor="@color/lan_dan"
        android:textSize="@dimen/_24px_in720p" />

    <LinearLayout
        android:id="@+id/imglay"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_10px_in720p"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center">

            <ImageView
                android:id="@+id/img1"
                android:layout_width="@dimen/_80px_in720p"
                android:layout_height="@dimen/_80px_in720p"
                 />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center">

            <ImageView
                android:id="@+id/img2"
                android:layout_width="@dimen/_80px_in720p"
                android:layout_height="@dimen/_80px_in720p"
                />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center">

            <ImageView
                android:id="@+id/img3"
                android:layout_width="@dimen/_80px_in720p"
                android:layout_height="@dimen/_80px_in720p"
                 />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center">

            <ImageView
                android:id="@+id/img4"
                android:layout_width="@dimen/_80px_in720p"
                android:layout_height="@dimen/_80px_in720p"
                />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center">

            <ImageView
                android:id="@+id/img5"
                android:layout_width="@dimen/_80px_in720p"
                android:layout_height="@dimen/_80px_in720p"
                />
        </LinearLayout>
    </LinearLayout>


    <LinearLayout
        android:id="@+id/videolay"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_10px_in720p"
        android:orientation="horizontal">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical">

            <TextView
                android:id="@+id/video"
                android:layout_marginLeft="@dimen/_20px_in720p"
                android:layout_width="@dimen/_80px_in720p"
                android:layout_height="@dimen/_80px_in720p"
                />
            <TextView
                android:id="@+id/videoplay"
                android:layout_marginLeft="@dimen/_20px_in720p"
                android:layout_width="@dimen/_80px_in720p"
                android:layout_height="@dimen/_80px_in720p"
                android:background="#cccccc"
                android:visibility="gone"
                />
        </RelativeLayout>


    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/_2px_in720p"
        android:layout_marginTop="@dimen/_20px_in720p"
        android:background="@color/white"/>
</LinearLayout>
