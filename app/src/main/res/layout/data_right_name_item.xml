<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal">

    <TextView
        android:layout_width="@dimen/_10px_in720p"
        android:layout_height="match_parent" />

    <LinearLayout
        android:background="@drawable/data_right_bg"
        android:layout_width="@dimen/_160px_in720p"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/_80px_in720p"
            android:background="@drawable/data_right_bg"
            android:gravity="center">

            <ImageView
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:id="@+id/state"
                android:layout_width="@dimen/_60px_in720p"
                android:layout_height="@dimen/_60px_in720p"
                android:src="@drawable/state_1" />

            <ImageView
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:id="@+id/img"
                android:layout_width="@dimen/_36px_in720p"
                android:layout_height="@dimen/_40px_in720p"
                android:src="@drawable/data_0_0" />

        </RelativeLayout>

        <TextView
            android:id="@+id/name"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_80px_in720p"
            android:background="@drawable/data_right_bg"
            android:gravity="center"
            android:maxLines="1"
            android:text="89"
            android:textColor="@color/white"
            android:textSize="@dimen/_28px_in720p" />

        <TextView
            android:id="@+id/tn"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_80px_in720p"
            android:background="@drawable/data_right_bg"
            android:gravity="center"
            android:maxLines="1"
            android:text="89"
            android:textColor="@color/white"
            android:textSize="@dimen/_28px_in720p" />

        <TextView
            android:id="@+id/jiankong"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_80px_in720p"
            android:background="@drawable/data_right_bg"
            android:gravity="center"
            android:maxLines="1"
            android:text="89"
            android:textColor="@color/white"
            android:textSize="@dimen/_28px_in720p" />

        <TextView
            android:id="@+id/zaixian"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_80px_in720p"
            android:background="@drawable/data_right_bg"
            android:gravity="center"
            android:maxLines="1"
            android:text="89"
            android:textColor="@color/white"
            android:textSize="@dimen/_28px_in720p" />

        <TextView
            android:id="@+id/qujian"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_80px_in720p"
            android:background="@drawable/data_right_bg"
            android:gravity="center"
            android:maxLines="1"
            android:text="89"
            android:textColor="@color/white"
            android:textSize="@dimen/_20px_in720p" />

        <TextView
            android:id="@+id/lon"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_80px_in720p"
            android:background="@drawable/data_right_bg"
            android:gravity="center"
            android:maxLines="1"
            android:text="89"
            android:textColor="@color/white"
            android:textSize="@dimen/_28px_in720p" />

        <TextView
            android:id="@+id/lasttime"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_80px_in720p"
            android:background="@drawable/data_right_bg"
            android:gravity="center"
            android:maxLines="1"
            android:text="89"
            android:textColor="@color/white"
            android:textSize="@dimen/_28px_in720p" />

        <TextView
            android:id="@+id/speed"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_80px_in720p"
            android:background="@drawable/data_right_bg"
            android:gravity="center"
            android:maxLines="1"
            android:text="89"
            android:textColor="@color/white"
            android:textSize="@dimen/_28px_in720p" />

        <TextView
            android:id="@+id/weihu"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_80px_in720p"
            android:background="@drawable/data_right_bg"
            android:gravity="center"
            android:maxLines="1"
            android:text="89"
            android:textColor="@color/white"
            android:textSize="@dimen/_28px_in720p" />

        <TextView
            android:id="@+id/time"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_80px_in720p"
            android:background="@drawable/data_right_bg"
            android:gravity="center"
            android:maxLines="2"
            android:text="89"
            android:textColor="@color/white"
            android:textSize="@dimen/_28px_in720p" />


    </LinearLayout>

    <TextView
        android:layout_width="@dimen/_10px_in720p"
        android:layout_height="match_parent" />


</LinearLayout>